{
  "webroot": "wwwroot",
  "version": "1.0.0-*",

  "dependencies": {
    "Swashbuckle": "6.0.0-beta1",
    "System.Reflection": "4.0.10-beta-22821",
    "System.Xml.XPath.XmlDocument": "4.0.0-beta-22821",
    "System.Reflection.TypeExtensions": "4.0.0-beta-22821",
    "System.Reflection.Extensions": "4.0.0-beta-22821",
    "Microsoft.AspNet.Mvc": "6.0.0-beta4",
    "Microsoft.AspNet.StaticFiles": "1.0.0-beta4",
    "Microsoft.AspNet.Server.IIS": "1.0.0-beta4",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-beta4",
    "EntityFramework.Core": "7.0.0-beta4",
    "EntityFramework.SqlServer": "7.0.0-beta4",
    "EntityFramework.Commands": "7.0.0-beta4"
  },

  "commands": {
    "web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:5000",
    "ef": "EntityFramework.Commands"
  },

  "frameworks": {
    "dnx451": { },
    "dnxcore50": { }
  },

  "publishExclude": [
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ],
  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ]
}
